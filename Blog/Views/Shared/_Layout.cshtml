<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="https://fonts.googleapis.com/css?family=Josefin+Sans:300, 400,700" rel="stylesheet">
    <link rel="stylesheet" href="~/Content/lib/bootstrapv4/css/bootstrap.css">
    <link rel="stylesheet" href="~/Content/lib/bootstrapv4/css/animate.css">
    <link rel="stylesheet" href="~/Content/lib/bootstrapv4/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/Content/lib/bootstrapv4/css/style.css">
    <link rel="stylesheet" href="~/Content/lib/bootstrapv4/fonts/ionicons/css/ionicons.min.css">
    <link rel="stylesheet" href="~/Content/lib/bootstrapv4/fonts/fontawesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/Content/lib/bootstrapv4/fonts/flaticon/font/flaticon.css">
    @RenderSection("customStyles",false)
</head>
<body>
    @Html.Partial("_Navbar")
    @RenderBody()
    @Html.Partial("_Footer")
    <script src="~/Content/lib/jquery/jquery-3.2.1.min.js"></script>
    <script src="~/Content/lib/jquery/jquery-migrate-3.0.0.js"></script>
    <script src="~/Content/lib/bootstrapv4/js/popper.min.js"></script>
    <script src="~/Content/lib/bootstrapv4/js/bootstrap.min.js"></script>
    <script src="~/Content/lib/bootstrapv4/js/owl.carousel.min.js"></script>
    <script src="~/Content/lib/jquery/jquery.waypoints.min.js"></script>
    <script src="~/Content/lib/jquery/jquery.stellar.min.js"></script>
    <script src="~/Content/lib/bootstrapv4/js/main.js"></script>
    @RenderSection("customScripts",false)
</body>
</html>
